{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-multi-str": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "max-len": "warn",
        "no-case-declarations": "off",
        "no-await-in-loop": "off",
        "class-methods-use-this": "off",
        "no-console": "off",
        "no-new": "off",
        "import/prefer-default-export": "off"
    }
}
